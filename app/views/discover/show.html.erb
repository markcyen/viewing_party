<h1>Welcome, <%= @user.email %>!</h1>

<h4><%= link_to "Log Out", "/logout", method: :delete %></h4>

<h2><%= button_to "Find Top Rated Movies", '/discover', method: :get %></h2>


<%= form_with url: "/discover", method: :get, local: true do |f| %>
  <%= f.label :search %>
  <%= f.text_field :search %>
  <%= f.submit "Find Movies", form: {style: 'display:inline-block;'} %>
<% end %>





